{
  "session_dir": "pretrained",
  "input_adapter": {
    "class": "examples.language_translation.adapters.InputAdapter"
  },
  "data_loaders": {},
  "pipelines": {
    "encoder_decoder": {
      "batch_adapter": {
        "class": "examples.language_translation.adapters.InferenceAdapter",
        "kwargs": {"hidden_size": 32}
      },
      "neural_graph": [{
        "model_name": "encoder",
        "inputs": ["x", "h"],
        "outputs": ["outputs", "h_e"],
        "optimizer_name": "encoder"
      },{
        "model_name": "decoder",
        "inputs": ["sos", "h_e"],
        "outputs": ["y_hat"],
        "optimizer_name": "decoder"
      }]
    }
  },
  "results": ["y_hat"],
  "post_processor": {
    "class": "examples.language_translation.postprocessors.PostProcessor"
  },
  "output_device": {
    "class": "examples.language_translation.output_devices.Printer"
  }
}